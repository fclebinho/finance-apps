{
  "version": 2,
  "env": {
      "POSTGRES_URL": "postgres://root@localhost:5432/finance_api",

      "KAFKA_CLIENT_ID": "kafkaSample",
      "KAFKA_BROKERS": "localhost:29092",
      "KAFKA_GROUP_ID": "my-kafka-consumer"
  },
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "DELETE"
          ]
      }
  ]
}
